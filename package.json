{
  "name": "pump-landing",
  "version": "1.0.0",
  "description": "Pump.fun Landing Page with dynamic token loading",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "proxy": "node proxy-server.js",
    "dev": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "push": "powershell -ExecutionPolicy Bypass -File upload-to-github.ps1",
    "push:msg": "powershell -ExecutionPolicy Bypass -File upload-to-github.ps1 -Message",
    "update": "bash update-on-server.sh"
  },
  "keywords": [
    "pump.fun",
    "solana",
    "crypto",
    "landing-page"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
