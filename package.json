{
  "name": "pump-landing",
  "version": "1.0.0",
  "description": "Pump.fun Landing Page with dynamic token loading",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "proxy": "node proxy-server.js",
    "dev": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "obfuscate": "node obfuscate.js",
    "obfuscate:full": "bash obfuscate-simple.sh",
    "obfuscate:check": "ls -lh js/*.obf.js 2>/dev/null || echo 'No obfuscated files found. Run: npm run obfuscate:full'"
  },
  "keywords": [
    "pump.fun",
    "solana",
    "crypto",
    "landing-page"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "sharp": "^0.33.0"
  }
}
